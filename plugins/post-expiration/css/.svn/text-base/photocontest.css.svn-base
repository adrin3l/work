#close_photo_fancy{
    background-image: url("/SysRes/GLOBALSkin/Libs/JQuery/Plugins/Fancybox/fancybox.png");
    background-position: -40px 0;
    cursor: pointer;
/*     display: none; */
    height: 30px;
    position: absolute;
    right: 35px;
    top: 10px;
    width: 30px;
    z-index: 1103;
}

#sms_vote_form {
	position: relative;
	width: 820px;
	height: 309px;
	padding: 20px 60px 20px 20px;
	z-index: 9999;
	background: url(/wp-content/plugins/vmh-photo-contest/css/images/modal-view2.png) no-repeat;
	text-shadow: 1px 1px 1px #333;
	font-size: 18px;
	color: #fff;
}
#sms_vote_form strong {
	font-weight: bold;
}
#sms_vote_form ul {
	list-style: disc;
	margin-left: 16px;
}
#sms_vote_form ul li {
	margin: 0 0 15px;
}
#mesaj_vot {
	margin: 0 0 20px;
}
#validate_vote {
	position: absolute;
	top: 270px;
	left: 100px;
}
#sms_code {
	height: 30px;
	line-height: 30px;
	width: 150px;
	border: none;
	font-size: 18px;
	padding: 0 5px;
	margin-right: 15px;
}
#activate_sms_code {
	height: 30px;
	line-height: 30px;
	font-size: 18px;
	background: #eee;
	background: -moz-linear-gradient(top, #fff, #d8d8d8);
	background: -webkit-linear-gradient(top, #fff, #d8d8d8);
	background: -ms-linear-gradient(top, #fff, #d8d8d8);
	background: -o-linear-gradient(top, #fff, #d8d8d8);
	background: linear-gradient(top, #fff, #d8d8d8);
	border: none;
	width: 100px;
	text-align: center;
	border-radius: 5px;
	margin-right: 80px;
}
#sms_code_error{
	left: 100px;
	position: absolute;
	top: 240px;
}
#sms_spinner{
left: 300px;
position: absolute;
top: 5px;
}
ul.related_links{
	padding : 20px 30px;
	color: #FFFFFF;
    font-size: 18px;
	list-style:disc;
}
ul.related_links li{
	line-height:30px;
	}
ul.related_links li a{
	color: #FFFFFF;
	text-decoration:none;
}
ul.related_links li a:hover,ul.related_links li a:active{
	color: #000;
	text-decoration:underline;
}
a img {
	border: none;
}
#gallery_list {
	float: left;
	width: 940px;
	list-style: none;
	margin: 0;
	padding: 0 0 0 20px;
        background-color: white;
}
#gallery_list li {
	float: left; 
	width: 250px; 
	margin-right: 15px; 
	margin-bottom: 17px;
}
#gallery_list li.last {
	margin-right: 0;
}
#gallery_list li h2 {
	margin: 15px 0 5px;
	font-size: 24px;
}
#miss_regulament {
	float: left;
	width: 960px;
	text-align: center;
}

.Button128x30 {
    margin-top: 10px;
    margin-left: 30px;
}
.RessortBox .LeftColumn .RessortControl h2 {
    height: 150px;
}
